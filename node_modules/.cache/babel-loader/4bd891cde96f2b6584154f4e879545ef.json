{"ast":null,"code":"import food from '../food.json';\nexport default {\n  name: 'HomeView',\n\n  data() {\n    return {\n      inventory: food\n    };\n  },\n\n  components: {}\n};","map":{"version":3,"mappings":"AA2DA,OAAOA,IAAP,MAAiB,cAAjB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;;AAEbC,MAAG,GAAK;AACN,WAAO;AACLC,eAAS,EAAEH;AADN,KAAP;AAGD,GANY;;AAObI,YAAU,EAAE;AAPC,CAAf","names":["food","name","data","inventory","components"],"sourceRoot":"","sources":["/Users/zhaorigetu.bao/vue_wp_01/product-and-cart/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div class=\"splash-container\">\n      <div class=\"splash\">\n        <h1>Splendid Food</h1>\n      </div>\n    </div>\n\n    <main class=\"wrapper\">\n\n      <h2>Recommended</h2>\n\n      <div class=\"recommended\">\n\n        <div v-for=\"(product, i) in inventory.slice(0, 3)\" :key=\"i\" class=\"card\">\n          <div class=\"card-title\">\n            {{ product.name }}\n          </div>\n          <div class=\"card-body\">\n            <i class=\"icofont-10x icofont-{{ product.icon }}\"></i>\n            <form>\n              <div class=\"row\">\n                <div class=\"cell\">\n                  <label>Type:</label>\n                </div>\n                <div class=\"cell\">\n                  <em>{{ product.type }}</em>\n                </div>\n              </div>\n              <div class=\"row\">\n                <div class=\"cell\">\n                  <label>Price:</label>\n                </div>\n                <div class=\"cell\">\n                  ${{ product.price.USD }}\n                </div>\n              </div>\n              <div class=\"row\">\n                <div class=\"cell\">\n                  <label>Quantity:</label>\n                </div>\n                <div class=\"cell\">\n                  <input type=\"number\" v-model.number=\"product.quantity\">\n                </div>\n              </div>\n            </form>\n          </div>\n          <div class=\"card-footer\">\n            <button @click=\"addToCart(product.name, i)\" class=\"btn btn-light\">\n              Add to cart\n            </button>\n          </div>\n        </div>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport food from '../food.json'\n\nexport default {\n  name: 'HomeView',\n  data () {\n    return {\n      inventory: food\n    }\n  },\n  components: {\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}