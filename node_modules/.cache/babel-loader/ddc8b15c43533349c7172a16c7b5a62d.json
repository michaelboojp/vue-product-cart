{"ast":null,"code":"import SidebarComponent from '@/components/SidebarComponent.vue';\nimport food from './food.json';\nexport default {\n  components: {\n    SidebarComponent\n  },\n\n  data() {\n    return {\n      showSideBar: false,\n      inventory: food,\n      cart: {}\n    };\n  },\n\n  computed: {\n    totalQuantaty() {\n      return Object.values(this.cart).reduce((acc, curr) => {\n        return acc + curr;\n      }, 0);\n    }\n\n  },\n  methods: {\n    addToCart(name, index) {\n      if (!this.cart[name]) this.cart[name] = 0;\n      this.cart[name] += this.inventory[index].quantity;\n      this.inventory[index].quantity = 0;\n      console.log(this.cart);\n    },\n\n    toggSideBar() {\n      this.showSideBar = !this.showSideBar;\n    },\n\n    removeItem(name) {\n      delete this.cart[name];\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2DA,OAAOA,gBAAP,MAA6B,mCAA7B;AACA,OAAOC,IAAP,MAAiB,aAAjB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVF;AADU,GADC;;AAIbG,MAAG,GAAK;AACN,WAAO;AACLC,iBAAW,EAAE,KADR;AAELC,eAAS,EAAEJ,IAFN;AAGLK,UAAI,EAAE;AAHD,KAAP;AAKD,GAVY;;AAWbC,UAAQ,EAAE;AACRC,iBAAY,GAAK;AACf,aAAOC,MAAM,CAACC,MAAP,CAAc,KAAKJ,IAAnB,EAAyBK,MAAzB,CAAgC,CAACC,GAAD,EAAMC,IAAN,KAAe;AACpD,eAAOD,GAAE,GAAIC,IAAb;AACD,OAFM,EAEJ,CAFI,CAAP;AAGF;;AALQ,GAXG;AAmBbC,SAAO,EAAE;AACPC,aAAQ,CAAGC,IAAH,EAASC,KAAT,EAAgB;AACtB,UAAI,CAAC,KAAKX,IAAL,CAAUU,IAAV,CAAL,EAAsB,KAAKV,IAAL,CAAUU,IAAV,IAAkB,CAAlB;AACtB,WAAKV,IAAL,CAAUU,IAAV,KAAmB,KAAKX,SAAL,CAAeY,KAAf,EAAsBC,QAAzC;AACA,WAAKb,SAAL,CAAeY,KAAf,EAAsBC,QAAtB,GAAiC,CAAjC;AACAC,aAAO,CAACC,GAAR,CAAY,KAAKd,IAAjB;AACD,KANM;;AAOPe,eAAU,GAAK;AACb,WAAKjB,WAAL,GAAmB,CAAC,KAAKA,WAAzB;AACD,KATM;;AAUPkB,cAAS,CAAGN,IAAH,EAAS;AAChB,aAAO,KAAKV,IAAL,CAAUU,IAAV,CAAP;AACF;;AAZO;AAnBI,CAAf","names":["SidebarComponent","food","components","data","showSideBar","inventory","cart","computed","totalQuantaty","Object","values","reduce","acc","curr","methods","addToCart","name","index","quantity","console","log","toggSideBar","removeItem"],"sourceRoot":"","sources":["/Users/zhaorigetu.bao/vue_wp_01/product-and-cart/src/components/SidebarComponent.vue"],"sourcesContent":["<template>\n  <aside class=\"cart-container\">\n    <div class=\"cart\">\n      <h1 class=\"cart-title spread\">\n        <span>\n          Cart\n          <i class=\"icofont-cart-alt icofont-1x\"></i>\n        </span>\n        <button @click=\"toggle()\" class=\"cart-close\">&times;</button>\n      </h1>\n\n      <div class=\"cart-body\">\n        <table class=\"cart-table\">\n          <thead>\n            <tr>\n              <th>\n                <span class=\"sr-only\">Product Image</span>\n              </th>\n              <th>Product</th>\n              <th>Price</th>\n              <th>Qty</th>\n              <th>Total</th>\n              <th>\n                <span class=\"sr-only\">Actions</span>\n              </th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"(quantity, key, i) in cart\" :key=\"i\">\n              <td>\n                <i class=\"icofont-carrot icofont-3x\"></i>\n              </td>\n              <td>{{ key }}</td>\n              <td>\\${{ getPrice(key) }}</td>\n              <td class=\"center\">{{ key }}</td>\n              <td>\\${{ (quantity * getPrice(key)).toFixed(2) }}</td>\n              <td class=\"center\">\n                <button @click=\"remove(key)\" class=\"btn btn-light cart-remove\">&times;</button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n\n        <p v-if=\"!Object.keys(cart).length\" class=\"center\">\n          <em>No items in cart</em>\n        </p>\n        <div class=\"spread\">\n          <span>\n            <strong>Total:</strong>\n            \\${{ calculateTotal() }}\n          </span>\n          <button class=\"btn btn-light\">Checkout</button>\n        </div>\n      </div>\n    </div>\n  </aside>\n</template>\n\n<script>\nimport SidebarComponent from '@/components/SidebarComponent.vue'\nimport food from './food.json'\n\nexport default {\n  components: {\n    SidebarComponent\n  },\n  data () {\n    return {\n      showSideBar: false,\n      inventory: food,\n      cart: {}\n    }\n  },\n  computed: {\n    totalQuantaty () {\n      return Object.values(this.cart).reduce((acc, curr) => {\n        return acc + curr\n      }, 0)\n    }\n\n  },\n  methods: {\n    addToCart (name, index) {\n      if (!this.cart[name]) this.cart[name] = 0\n      this.cart[name] += this.inventory[index].quantity\n      this.inventory[index].quantity = 0\n      console.log(this.cart)\n    },\n    toggSideBar () {\n      this.showSideBar = !this.showSideBar\n    },\n    removeItem (name) {\n      delete this.cart[name]\n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}