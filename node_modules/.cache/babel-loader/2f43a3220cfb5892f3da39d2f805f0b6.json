{"ast":null,"code":"import SidebarComponent from '@/components/SidebarComponent.vue';\nimport food from './food.json';\nexport default {\n  components: {\n    SidebarComponent\n  },\n\n  data() {\n    return {\n      showSideBar: false,\n      inventory: food,\n      cart: {}\n    };\n  },\n\n  computed: {\n    totalQuantaty() {\n      return Object.values(this.cart).reduce((acc, curr) => {\n        return acc + curr;\n      }, 0);\n    }\n\n  },\n  methods: {\n    addToCart(name, quantity) {\n      if (!this.cart[name]) this.cart[name] = 0;\n      this.cart[name] += quantity;\n    },\n\n    toggSideBar() {\n      this.showSideBar = !this.showSideBar;\n    },\n\n    removeItem(name) {\n      delete this.cart[name];\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwCA,OAAOA,gBAAP,MAA6B,mCAA7B;AACA,OAAOC,IAAP,MAAiB,aAAjB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVF;AADU,GADC;;AAIbG,MAAG,GAAK;AACN,WAAO;AACLC,iBAAW,EAAE,KADR;AAELC,eAAS,EAAEJ,IAFN;AAGLK,UAAI,EAAE;AAHD,KAAP;AAKD,GAVY;;AAWbC,UAAQ,EAAE;AACRC,iBAAY,GAAK;AACf,aAAOC,MAAM,CAACC,MAAP,CAAc,KAAKJ,IAAnB,EAAyBK,MAAzB,CAAgC,CAACC,GAAD,EAAMC,IAAN,KAAe;AACpD,eAAOD,GAAE,GAAIC,IAAb;AACD,OAFM,EAEJ,CAFI,CAAP;AAGF;;AALQ,GAXG;AAmBbC,SAAO,EAAE;AACPC,aAAQ,CAAGC,IAAH,EAASC,QAAT,EAAmB;AACzB,UAAI,CAAC,KAAKX,IAAL,CAAUU,IAAV,CAAL,EAAsB,KAAKV,IAAL,CAAUU,IAAV,IAAkB,CAAlB;AACtB,WAAKV,IAAL,CAAUU,IAAV,KAAmBC,QAAnB;AACD,KAJM;;AAKPC,eAAU,GAAK;AACb,WAAKd,WAAL,GAAmB,CAAC,KAAKA,WAAzB;AACD,KAPM;;AAQPe,cAAS,CAAGH,IAAH,EAAS;AAChB,aAAO,KAAKV,IAAL,CAAUU,IAAV,CAAP;AACF;;AAVO;AAnBI,CAAf","names":["SidebarComponent","food","components","data","showSideBar","inventory","cart","computed","totalQuantaty","Object","values","reduce","acc","curr","methods","addToCart","name","quantity","toggSideBar","removeItem"],"sourceRoot":"","sources":["/Users/zhaorigetu.bao/vue_wp_01/product-and-cart/src/App.vue"],"sourcesContent":["<template>\n  <header class=\"top-bar spread\">\n    <nav class=\"top-bar-nav\">\n\n      <router-link to=\"/\" class=\"top-bar-link\">\n        <i class=\"icofont-spoon-and-fork\"></i>\n        <span>Home</span>\n      </router-link>\n      <router-link to=\"/home\" class=\"top-bar-link\">\n        <i class=\"icofont-spoon-and-fork\"></i>\n        <span>Home</span>\n      </router-link>\n      <router-link to=\"/products\" class=\"top-bar-link\">\n        <span>Products</span>\n      </router-link>\n      <router-link to=\"/past-orders\" class=\"top-bar-link\">\n        <span>Past Orders</span>\n      </router-link>\n    </nav>\n    <a @click=\"toggSideBar()\" class=\"top-bar-cart-link\">\n      <i class=\"icofont-cart-alt icofont-1x\"></i>\n      <span>Cart ({{ totalQuantaty}})</span>\n    </a>\n  </header>\n\n  <!-- <nav>\n    <router-link to=\"/\">Home</router-link> |\n    <router-link to=\"/about\">About</router-link>\n  </nav>-->\n  <router-view :inventory=\"inventory\" :addToCart=\"addToCart\" />\n  <SidebarComponent\n    v-if=\"showSideBar\"\n    :toggle=\"toggSideBar\"\n    :cart=\"cart\"\n    :inventory=\"inventory\"\n    :remove=\"removeItem\"\n  />\n</template>\n\n<script>\nimport SidebarComponent from '@/components/SidebarComponent.vue'\nimport food from './food.json'\n\nexport default {\n  components: {\n    SidebarComponent\n  },\n  data () {\n    return {\n      showSideBar: false,\n      inventory: food,\n      cart: {}\n    }\n  },\n  computed: {\n    totalQuantaty () {\n      return Object.values(this.cart).reduce((acc, curr) => {\n        return acc + curr\n      }, 0)\n    }\n\n  },\n  methods: {\n    addToCart (name, quantity) {\n      if (!this.cart[name]) this.cart[name] = 0\n      this.cart[name] += quantity\n    },\n    toggSideBar () {\n      this.showSideBar = !this.showSideBar\n    },\n    removeItem (name) {\n      delete this.cart[name]\n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}