{"ast":null,"code":"import SidebarComponent from '@/components/SidebarComponent.vue';\nimport food from '../food.json';\nexport default {\n  components: {\n    SidebarComponent\n  },\n\n  data() {\n    return {\n      showSideBar: false,\n      inventory: food,\n      cart: {}\n    };\n  },\n\n  computed: {\n    totalQuantaty() {\n      return Object.values(this.cart).reduce((acc, curr) => {\n        return acc + curr;\n      }, 0);\n    }\n\n  },\n  methods: {\n    addToCart(name, index) {\n      if (!this.cart[name]) this.cart[name] = 0;\n      this.cart[name] += this.inventory[index].quantity;\n      this.inventory[index].quantity = 0;\n      console.log(this.cart);\n    },\n\n    toggSideBar() {\n      this.showSideBar = !this.showSideBar;\n    },\n\n    removeItem(name) {\n      delete this.cart[name];\n    }\n\n  },\n\n  async mounted() {\n    const data = await food.json();\n    this.inventory = data;\n  }\n\n};","map":{"version":3,"mappings":"AAmCA,OAAOA,gBAAP,MAA6B,mCAA7B;AACA,OAAOC,IAAP,MAAiB,cAAjB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVF;AADU,GADC;;AAIbG,MAAG,GAAK;AACN,WAAO;AACLC,iBAAW,EAAE,KADR;AAELC,eAAS,EAAEJ,IAFN;AAGLK,UAAI,EAAE;AAHD,KAAP;AAKD,GAVY;;AAWbC,UAAQ,EAAE;AACRC,iBAAY,GAAK;AACf,aAAOC,MAAM,CAACC,MAAP,CAAc,KAAKJ,IAAnB,EAAyBK,MAAzB,CAAgC,CAACC,GAAD,EAAMC,IAAN,KAAe;AACpD,eAAOD,GAAE,GAAIC,IAAb;AACD,OAFM,EAEJ,CAFI,CAAP;AAGF;;AALQ,GAXG;AAmBbC,SAAO,EAAE;AACPC,aAAQ,CAAGC,IAAH,EAASC,KAAT,EAAgB;AACtB,UAAI,CAAC,KAAKX,IAAL,CAAUU,IAAV,CAAL,EAAsB,KAAKV,IAAL,CAAUU,IAAV,IAAkB,CAAlB;AACtB,WAAKV,IAAL,CAAUU,IAAV,KAAmB,KAAKX,SAAL,CAAeY,KAAf,EAAsBC,QAAzC;AACA,WAAKb,SAAL,CAAeY,KAAf,EAAsBC,QAAtB,GAAiC,CAAjC;AACAC,aAAO,CAACC,GAAR,CAAY,KAAKd,IAAjB;AACD,KANM;;AAOPe,eAAU,GAAK;AACb,WAAKjB,WAAL,GAAmB,CAAC,KAAKA,WAAzB;AACD,KATM;;AAUPkB,cAAS,CAAGN,IAAH,EAAS;AAChB,aAAO,KAAKV,IAAL,CAAUU,IAAV,CAAP;AACF;;AAZO,GAnBI;;AAiCb,QAAMO,OAAN,GAAiB;AACf,UAAMpB,IAAG,GAAI,MAAMF,IAAI,CAACuB,IAAL,EAAnB;AACA,SAAKnB,SAAL,GAAiBF,IAAjB;AACF;;AApCa,CAAf","names":["SidebarComponent","food","components","da