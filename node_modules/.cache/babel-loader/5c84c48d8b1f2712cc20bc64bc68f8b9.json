deBar () {\n      this.showSideBar = !this.showSideBar\n    },\n    removeItem (name) {\n      delete this.cart[name]\n    }\n  },\n  async mounted () {\n    const data = await food.json()\n    this.inventory = data\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}